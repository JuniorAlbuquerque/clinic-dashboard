{
  "name": "boilerplate-react-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:open": "vite --open",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:3000/graphql schema-temp.gql",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema-temp.gql --tagName=gql --outputFlat src/graphql/generated"
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@formkit/auto-animate": "^1.0.0-beta.3",
    "@headlessui/react": "^1.7.3",
    "@heroicons/react": "^2.0.11",
    "@hookform/resolvers": "^2.9.10",
    "clsx": "^1.2.1",
    "date-fns": "^2.29.3",
    "framer-motion": "^7.6.1",
    "graphql": "^15.3.0",
    "mask-hooks": "^2.8.1",
    "react": "^18.2.0",
    "react-big-calendar": "^1.5.2",
    "react-currency-input-field": "^3.6.10",
    "react-date-picker": "^9.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.42.1",
    "react-router-dom": "^6.4.1",
    "react-toastify": "^9.0.8",
    "zod": "^3.20.2",
    "zustand": "^4.1.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.7.3",
    "@types/react": "^18.0.17",
    "@types/react-big-calendar": "^0.38.2",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "@vitejs/plugin-react": "^2.0.1",
    "@vitest/ui": "^0.21.1",
    "apollo": "^2.12.2",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^20.0.0",
    "postcss": "^8.4.17",
    "prettier": "^2.7.1",
    "tailwind-scrollbar": "^2.0.1",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.6.4",
    "vite": "^3.0.7",
    "vite-plugin-svgr": "^2.2.1",
    "vitest": "^0.21.1"
  }
}
